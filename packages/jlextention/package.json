{
  "name": "jupyterlab-threeplot",
  "version": "0.1.0",
  "description": "A JupyterLab extension for threeplot. A set of utility widgets for plotting with pythreejs",
  "main": "build/index.js",
  "typings": "build/index.d.ts",
  "dependencies": {
    "@jupyterlab/application": "~0.10.0",
    "@phosphor/coreutils": "~1.3.0",
    "@jupyter-widgets/jupyterlab-manager": "~0.27.0",
    "jupyter-threeplot": "^1.0.0",
    "jupyterlab-scales": "^0.1.1-0",
    "jupyterlab-threejs": "^0.1.0"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~2.4.2"
  },
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab extension",
    "jupyterlab notebook"
  ],
  "jupyterlab": {
    "extension": true
  },
  "scripts": {
    "clean": "rimraf build",
    "build": "tsc --project src",
    "prepublish": "npm run clean && npm run build"
  },
  "files": [
    "build/*.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/vidartf/threeplot"
  },
  "author": "A set of utility widgets for plotting with pythreejs",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/vidartf/threeplot/issues"
  },
  "homepage": "https://github.com/vidartf/threeplot"
}
